{
  "AWS::CloudFormation::Init": {
    "configSets": {
      "test1": ["1"],
      "test2": [{ "ConfigSet": "test1" }, "2"],
      "default": [{ "ConfigSet": "test2" }]
    },
    "1": {
      "commands": {
        "test": {
          "command": "echo \"$MAGIC\" > test.txt",
          "env": { "MAGIC": "I come from the environment!" },
          "cwd": "~"
        }
      }
    },
    "2": {
      "commands": {
        "test": {
          "command": "echo \"$MAGIC\" >> test.txt",
          "env": { "MAGIC": "I am test 2!" },
          "cwd": "~"
        }
      }
    }
  }
}
